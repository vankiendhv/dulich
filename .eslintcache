yenmai = (money, km) => {\r\n        return money - money * (km / 100);\r\n    };\r\n    const { name, sdt, diachi, email, nguoilon, treem, embe } = state;\r\n    const checkKhuyenmai = () => {\r\n        if (giakhuyenmai.Khuyenmais.length === 0) {\r\n            return giakhuyenmai.gianguoilon;\r\n        } else {\r\n            if (giakhuyenmai.Khuyenmais[0].status === 0) {\r\n                return giakhuyenmai.gianguoilon;\r\n            } else {\r\n                return tinhkhuyenmai(\r\n                    giakhuyenmai.gianguoilon,\r\n                    giakhuyenmai.Khuyenmais[0].khuyenmai,\r\n                );\r\n            }\r\n        }\r\n    };\r\n\r\n    const radioStyle = {\r\n        display: \"block\",\r\n        height: \"30px\",\r\n        lineHeight: \"30px\",\r\n    };\r\n    const adddate = (e) => {\r\n        setState({\r\n            ...state,\r\n            date: state.listdate.find((x) => x.id === +e).ngay,\r\n        });\r\n    };\r\n    var tong;\r\n    if (giakhuyenmai) {\r\n        tong = Number(nguoilon) + Number(treem) + Number(embe);\r\n    }\r\n\r\n    const handleChange = (value) => {\r\n        let serviceActive = services.filter(s => value.includes(s.id))\r\n        console.log(JSON.stringify(serviceActive?.map(s => ({ id: s.id, name: s.name, price: s.price }))));\r\n        let totalService = serviceActive.reduce((accumulator, current) => accumulator + current.price, 0)\r\n        setState({\r\n            ...state,\r\n            totalService,\r\n            serviceSaveHoadon: serviceActive,\r\n            serviceActive: value\r\n        })\r\n    };\r\n\r\n    const handleOnOkHotel = (value) => {\r\n\r\n        setState({\r\n            ...state,\r\n            roomActive: value\r\n        })\r\n        setIsModalHotel(false)\r\n    }\r\n\r\n    const getTotalPrice = (data) => {\r\n        return data.reduce((a, b) => (a + b.price), 0)\r\n    }\r\n\r\n    return (\r\n        <div id=\"detail-tour\">\r\n            <div class